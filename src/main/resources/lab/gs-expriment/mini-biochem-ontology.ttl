	rdfs:subClassOf :E10 .
:E105 
	rdf:type owl:Class ;
	rdfs:subClassOf :E10 .	
:E109 
	rdf:type owl:Class ;
	rdfs:subClassOf :E10 .

:E11 rdf:type owl:Class .
:E111 
	rdf:type owl:Class ;
	rdfs:subClassOf :E11 .
:E115 
	rdf:type owl:Class ;
	rdfs:subClassOf :E11 .
	
:icd9_2500
	rdf:type owl:Class ;
	rdfs:subClassOf :E11 .
:icd9_25009
	rdf:type owl:Class ;
